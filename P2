class fibonacci {
public static void main (String args[]) {
int a=0,b=1,c,number;
number=Integer.parseInt(args [0]);
System.out.println(a);
System.out.println(b);
for (int i=1;i<=(number-2;i++)
{
c=a+b;
System.out.println(c);
a=b;
b=c;
}}}

